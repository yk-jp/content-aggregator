{
  "name": "content-aggregator-web",
  "version": "0.1.0",
  "private": true,
  "@scripts-comments": {
    "postinstall": "To automatically have Git hooks enabled after install husky"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-format": "npx prettier --check .",
    "check-types": "tsc --pretty --noEmit",
    "reformat": "npx prettier --write .",
    "fix-lint": "next lint --fix",
    "postinstall": "cd .. && husky install web/.husky"
  },
  "dependencies": {
    "@types/node": "20.4.4",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "firebase": "^10.1.0",
    "next": "13.4.12",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3"
  }
}
